      <output escape="text" type="literal">.</output>
    </case>
    <case branch-values='a:1:{i:0;s:1:"c";}' test=".='c'">
      <output escape="text" type="literal">.</output>
    </case>
    <case branch-values='a:1:{i:0;s:1:"d";}' test=".='d'">
      <output escape="text" type="literal">.</output>
    </case>
  </switch>
</template>